{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "jsx": "react",
    "skipLibCheck": true,
    "baseUrl": ".",

    "noEmit": true
  },
  "include": [
    "src"
  ]
}
